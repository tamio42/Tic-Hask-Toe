{
  "workbench.colorTheme": "Solarized Light",
  "editor.fontSize": 16,
  "workbench.iconTheme": "vscode-icons",
  "haskell.plugin.ghcide-type-lenses.globalOn": false,
  "haskell.plugin.importLens.codeLensOn": false,
  "haskell.manageHLS": "GHCup",
  "gitpod.openInStable.neverPrompt": true,
  "redhat.telemetry.enabled": true,
  "vsicons.dontShowNewVersionMessage": true,
  "workbench.preferredDarkColorTheme": "Solarized Light",
  "workbench.preferredHighContrastColorTheme": "Solarized Light",
  "window.autoDetectColorScheme": true,
}